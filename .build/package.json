{
  "name": "@starbeam-workspace/build-support",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "starbeam": {
    "type": "support:build",
    "keep:js": true
  },
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    "default": "./index.js"
  },
  "dependencies": {
    "@types/rollup__plugin-commonjs": "workspace:^",
    "@types/rollup__plugin-node-resolve": "workspace:^",
    "rollup": "*",
    "magic-string": "*",
    "rollup-plugin-ts": "*",
    "rollup-plugin-postcss": "*",
    "@rollup/plugin-commonjs": "*",
    "@rollup/plugin-node-resolve": "*",
    "rollup-pluginutils": "^2.8.2"
  },
  "devDependencies": {
    "@types/node": "*",
    "typescript": "*"
  },
  "publishConfig": {
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "test:lint": "eslint",
    "test:build": "tsc -b"
  }
}
