{
  "name": "starbeam",
  "version": "0.0.0",
  "type": "module",
  "types": "src/index.ts",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "node ./jest/run.js",
    "lint": "eslint .",
    "typecheck": "tsc -b",
