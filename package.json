    "jsdom": "^19.0.0",
    "jsdom-quokka-plugin": "^1.0.16",
    "lodash.zip": "^4.2.0",
    "shelljs": "^0.8.5",
    "ts-jest": "^27.1.3",
    "typescript": "next"
