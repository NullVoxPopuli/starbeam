{
  "dependencies": {
    "@domtree/flavors": "workspace:^",
    "@starbeam/core-utils": "workspace:^",
    "@starbeam/interfaces": "workspace:^",
    "@starbeam/reactive": "workspace:^",
    "@starbeam/shared": "workspace:^",
    "@starbeam/tags": "workspace:^",
    "@starbeam/verify": "workspace:^",
    "buffer": "^6.0.3",
    "stacktracey": "^2.1.8"
  },
  "devDependencies": {
    "@starbeam-dev/build-support": "workspace:^",
    "@starbeam/eslint-plugin": "workspace:^",
    "@types/node": "18.16.1",
    "rollup": "^3.20.6"
  },
  "exports": {
    "default": "./index.ts"
  },
  "main": "index.ts",
  "name": "@starbeam/debug",
  "publishConfig": {
    "exports": {
      ".": {
        "default": "./dist/index.cjs",
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts"
  },
  "scripts": {
    "test:lint": "eslint \"index.ts\" \"src/**/*.ts\" --max-warnings 0",
    "test:specs": "vitest --run",
    "test:types": "tsc -b"
  },
  "starbeam": {
    "inline": [
      "stacktracey",
      "get-source",
      "source-map",
      "data-uri-to-buffer",
      "as-table",
      "printable-characters",
      "chalk"
    ],
    "type": "library:public"
  },
  "type": "module",
  "types": "index.ts",
  "version": "0.8.9"
}
