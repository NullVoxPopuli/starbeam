{
  "name": "@starbeam/core",
  "version": "0.7.3",
  "type": "module",
  "main": "index.ts",
  "dependencies": {
    "@starbeam/universal": "workspace:*"
  },
  "devDependencies": {
    "@starbeam/eslint-plugin": "workspace:^",
    "@starbeam-workspace/build-support": "workspace:^",
    "@typescript-eslint/eslint-plugin": "*"
  },
  "publishConfig": {
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      }
    }
  },
  "exports": {
    "default": "./index.ts"
  },
  "scripts": {
    "test:lint": "eslint",
    "test:specs": "vitest",
    "test:types": "tsc -b"
  },
  "starbeam:type": "library"
}
