{
  "name": "@starbeam/eslint-plugin",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "index.js",
  "private": true,
  "peerDependencies": {
    "eslint": "^8.26.0",
    "typescript": "*",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0"
  },
  "dependencies": {
